def warshall(matriz):
    n = len(matriz)
    alcance = [fila[:] for fila in matriz]

    for k in range(n):
        for i in range(n):
            for j in range(n):
                alcance[i][j] = alcance[i][j] or (alcance[i][k] and alcance[k][j])

    return alcance


# 1 = hay camino directo, 0 = no hay camino
matriz = [
    [1, 1, 0],
    [0, 1, 1],
    [0, 0, 1]
]

resultado = warshall(matriz)

print("Matriz de alcanzabilidad:")
for fila in resultado:
    print(fila)
